{
  "version": "1.0.0",
  "environment": "development",
  "api": {
    "baseUrl": "http://localhost:3000",
    "timeout": 10000,
    "retryAttempts": 2
  },
  "ui": {
    "theme": "light",
    "primaryColor": "#3B82F6",
    "secondaryColor": "#10B981",
    "fontFamily": "Inter, system-ui, sans-serif",
    "animationsEnabled": true
  },
  "features": {
    "enableDarkMode": true,
    "enableNotifications": true,
    "enableOfflineMode": false
  },
  "analytics": {
    "enabled": false,
    "trackingId": "REPLACE_WITH_ENV_VAR"
  },
  "seo": {
    "defaultTitle": "Protocol",
    "defaultDescription": "A modern web application",
    "defaultKeywords": "protocol, web, application",
    "defaultOgImage": "/images/og-image.png"
  },
  "cache": {
    "maxAge": 3600,
    "staleWhileRevalidate": true
  }
}
