{
  "version": "1.0.0",
  "environment": "development",
  "api": {
    "baseUrl": "http://localhost:3000",
    "timeout": 30000,
    "retryAttempts": 3
  },
  "database": {
    "type": "postgres",
    "host": "localhost",
    "port": 5432,
    "username": "postgres",
    "database": "protocol_db",
    "synchronize": true,
    "logging": true
  },
  "logging": {
    "level": "debug",
    "format": "json",
    "transports": ["console", "file"]
  },
  "security": {
    "jwtSecret": "REPLACE_WITH_ENV_VAR",
    "jwtExpiresIn": "1d",
    "bcryptSaltRounds": 10
  },
  "features": {
    "enableRegistration": true,
    "enablePasswordReset": true,
    "enableTwoFactorAuth": false
  }
}
